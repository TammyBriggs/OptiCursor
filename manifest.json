{
    "manifest_version": 3,
    "name": "Eye Tracking Chrome Extension",
    "version": "1.0.0",
    "description": "A Chrome extension to track eye movements using TensorFlow.js",
    "action": {
      "default_popup": "src/popup/popup.html",
      "default_icon": "src/assets/icon.png"
    },
    "permissions": [
      "scripting",
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "src/background/background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/content/content.js"]
      }
    ]
  }
  